  "homepage": "https://www.frameone.ch",
  "dependencies": {
    "gsap": "3.13.0",
    "lenis": "1.3.14",
    "lodash": "4.17.21",
    "normalize.css": "8.0.1"
  },
  "devDependencies": {
    "@christian-beckmann/parcel-transformer-twig": "0.1.1",
    "@parcel/resolver-glob": "2.16.1",
    "@parcel/transformer-sass": "2.16.1",
    "parcel": "2.16.1",
    "parcel-reporter-static-files-copy": "1.5.3",
    "prettier": "3.6.2",
    "prettier-plugin-twig-melody": "0.4.6",
    "stylelint": "16.19.1",
